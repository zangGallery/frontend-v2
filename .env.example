# Required: Alchemy API key for Base network (for contract reads)
GATSBY_ALCHEMY_BASE_API_KEY=your_alchemy_base_api_key_here

# Optional: Alchemy API key for Ethereum mainnet (for ENS resolution)
# If not set, falls back to public RPC (may have rate limits)
GATSBY_ALCHEMY_MAINNET_API_KEY=your_alchemy_mainnet_api_key_here

# Optional: Site URL for OpenGraph tags (defaults to https://zang.gallery)
SITE_URL=https://zang.gallery

# Server port (Railway sets this automatically)
PORT=3000
